    "globby": "^12.1.0",
    "husky": "^7.0.4",
    "next-images": "^1.8.4",
    "next-transpile-modules": "^9.0.0",
    "postcss": "^8.4.5",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^7.2.0",
