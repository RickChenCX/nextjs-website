  "scripts": {
    "dev": "cross-env NODE_ENV='local' node ./env.js && next dev",
    "build": "next build",
    "build:dev": "cross-env NODE_ENV='development' node ./env.js && next build",
    "build:test": "cross-env NODE_ENV='test' node ./env.js && next build",
