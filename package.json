    "antd": "^4.18.2",
    "antd-mobile": "^5.0.0-rc.21",
    "babel-plugin-import": "^1.13.3",
    "bad-words": "^3.0.4",
    "eslint-config-react-app": "^7.0.0",
    "jshint": "^2.13.3",
    "next": "12.0.8",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "^5.3.3",
    "swr": "^1.1.1"
  },
  "browserslist": [
    ">0.3%",
